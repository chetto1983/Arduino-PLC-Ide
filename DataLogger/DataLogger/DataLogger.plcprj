						<vars>
							<inputVars>
								<var name="AnaInput" type="REAL"/>
							</inputVars>
							<outputVars>
								<var name="T_Out" type="REAL"/>
							</outputVars>
							<localConsts>
								<const name="a" type="REAL">
									<initValue>0.0039083</initValue>
								</const>
								<const name="b" type="REAL">
									<initValue>-0.0000005775</initValue>
								</const>
							</localConsts>
						</vars>
						<iecDeclaration active="FALSE"/>

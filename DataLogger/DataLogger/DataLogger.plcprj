					<function name="DAY_OF_MONTH" version="1.0.0" creationDate="0" lastModifiedDate="0" excludeFromBuild="FALSE" excludeFromBuildIfNotDef="">
						<returnValue>INT</returnValue>
						<vars>
							<inputVars>
								<var name="IDATE" type="DATE"/>

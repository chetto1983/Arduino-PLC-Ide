						<returnValue>DATE</returnValue>
						<vars>
							<inputVars>
								<var name="year" type="INT"/>
							</inputVars>
							<localVars>
								<var name="b" type="INT"/>
								<var name="c" type="INT"/>
								<var name="oday" type="INT"/>
							</localVars>
